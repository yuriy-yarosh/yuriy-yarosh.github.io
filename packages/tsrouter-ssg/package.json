{
  "name": "@yaroshdev/tsrouter-ssg",
  "version": "0.1.0",
  "description": "yarosh.dev simple tanstack router ssg",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.7.0",
  "repository": {
    "type": "git",
